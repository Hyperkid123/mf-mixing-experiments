    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "swc-loader": "^0.2.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-top-level-await": "^1.4.1",
