  "scripts": {
    "build": "concurrently \"npm run build:webpack\" \"npm run build:rspack\" \"npm run build:vite\"",
    "build:webpack": "webpack --config webpack.config.js --mode production",
    "build:rspack": "rspack build -c ./rspack.config.js",
